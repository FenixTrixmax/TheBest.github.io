function LongJump()
if longjh == off then
longjh = on;
gg.setRanges(gg.REGION_ANONYMOUS) 
gg.searchNumber("0.375;0.74::5", gg.TYPE_FLOAT, false, gg.SIGN_EQUAL, 0, -1, 0)
gg.refineNumber("0.74", gg.TYPE_FLOAT) 
local t = gg.getResults(100, nil, nil, nil, nil, nil, nil, nil, nil)
gg.addListItems(t) t = nil local copy = false 
local t = gg.getListItems() 
if not copy 
then gg.removeListItems(t) end 
for i, v in ipairs(t) do
 if gg.getTargetInfo().x64
 then v.address = v.address + 0xffffffffffffffb4 else v.address = v.address + -56 end
 if copy then v.name = v.name..' #2' end end
gg.addListItems(t) t = nil copy = nil local 
t = gg.getListItems() for i, v in ipairs(t) do if v.flags == gg.TYPE_FLOAT then 
v.value = "0.2" 
v.freeze = true 
v.freezeType = gg.FREEZE_NORMAL
end end 
gg.addListItems(t) 
t = nil
gg.clearResults(true)
gg.toast("🛡️LongJump ON🛡️")
gg.sleep(1000)
gg.toast("【🔰Script by STARDUST🔰】")
gg.clearResults(true)
else if
longjh == on then
longjh = off;
gg.setRanges(gg.REGION_ANONYMOUS) 
gg.searchNumber("0.375;0.74::5", gg.TYPE_FLOAT, false, gg.SIGN_EQUAL, 0, -1, 0)
gg.refineNumber("0.74", gg.TYPE_FLOAT) 
local t = gg.getResults(100, nil, nil, nil, nil, nil, nil, nil, nil)
gg.addListItems(t) t = nil local copy = false 
local t = gg.getListItems() 
if not copy 
then gg.removeListItems(t) end 
for i, v in ipairs(t) do
 if gg.getTargetInfo().x64
 then v.address = v.address + 0xffffffffffffffb4 else v.address = v.address + -56 end
 if copy then v.name = v.name..' #2' end end
gg.addListItems(t) t = nil copy = nil local 
t = gg.getListItems() for i, v in ipairs(t) do if v.flags == gg.TYPE_FLOAT then 
v.value = "0.2" 
v.freeze = false 
end end 
gg.addListItems(t) 
t = nil
gg.clearResults(true)
gg.toast("🛡️LongJump OFF🛡️")
gg.sleep(1000)
gg.toast("【🔰Script by STARDUST🔰】")
gg.clearResults(true)
end
end
end



function WallShot()
if wallshoth == off then
wallshoth = on;
gg.setRanges(gg.REGION_ANONYMOUS)
      gg.searchNumber('h FB FF 5F 3F', gg.TYPE_BYTE)
      gg.getResults(229)      
      gg.editAll('h 7A 01 01 01', gg.TYPE_BYTE)
gg.clearResults(true)
gg.toast("🛡️WallShot ON🛡️")
gg.sleep(1000)
gg.toast("【🔰Script by STARDUST🔰】")
gg.clearResults(true)
else if
wallshoth == on then
wallshoth = off;
gg.setRanges(gg.REGION_ANONYMOUS)
      gg.searchNumber('h 7A 01 01 01', gg.TYPE_BYTE)
      gg.getResults(229)      
      gg.editAll('h FB FF 5F 3F', gg.TYPE_BYTE)
gg.clearResults(true)
gg.toast("🛡️WallShot OFF🛡️")
gg.sleep(1000)
gg.toast("【🔰Script by STARDUST🔰】")
gg.clearResults(true)
end
end
end



function SpeedHack()
if speedh == off then
speedh = on;
gg.setRanges(gg.REGION_ANONYMOUS) 
gg.searchNumber("h0300000030002E00", gg.TYPE_BYTE, false, gg.SIGN_EQUAL, 0, -1, 0)
gg.getResults(9999)
gg.editAll("h0300000030002E003200000000000000",gg.TYPE_BYTE)
gg.clearResults(true)
gg.toast("🛡️SpeedHack ON🛡️")
gg.sleep(1000)
gg.toast("【🔰Script by STARDUST🔰】")
gg.clearResults(true)
else if
speedh == on then
speedh = off;
gg.setRanges(gg.REGION_ANONYMOUS) 
gg.searchNumber("h0300000030002E003200000000000000", gg.TYPE_BYTE, false, gg.SIGN_EQUAL, 0, -1, 0)
gg.getResults(9999)
gg.editAll("h0300000030002E00",gg.TYPE_BYTE)
gg.clearResults(true)
gg.toast("🛡️SpeedHack OFF🛡️")
gg.sleep(1000)
gg.toast("【🔰Script by STARDUST🔰】")
gg.clearResults(true)
end
end
end





