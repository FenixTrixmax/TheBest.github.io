function Name()
gg.clearResults()
gg.setRanges(gg.REGION_ANONYMOUS)
local nik = gg.prompt({"Enter Your NickName:"},{"Your Name"},{"Text"})
local nik1 = gg.prompt({"Enter The NickName You Want To Change To:"},{"Your Name"},{"text"})
local nik2 = ";"..nik[1]
local nik3 = ";"..nik1[1]
gg.searchNumber(nik2, gg.TYPE_WORD)
gg.getResults(999)
gg.editAll(nik3, gg.TYPE_WORD)
gg.clearResults(true)
gg.toast("🛡️Nickname&Clan Hack ON🛡️")
gg.sleep(1000)
gg.toast("【🔰Script by STARDUST🔰】")
gg.clearResults(true)
end

