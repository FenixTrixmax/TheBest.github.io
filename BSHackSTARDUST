function onCreate()
gg.clearResults(true)
gg.setRanges(gg.REGION_CODE_APP)
gg.searchNumber('h FF C3 02 D1 E8 2B 00 FD FB 6B 06 A9 F9 63 07 A9 F7 5B 08 A9 F5 53 09 A9 F3 7B 0A A9', gg.TYPE_BYTE)
gg.getResults(1000)
gg.editAll('h C0 03 5F D6', gg.TYPE_BYTE)
gg.clearResults(true)
gg.toast('🔰Script by STARDUST🔰')
gg.sleep(100)
gg.alert("🔰Script by STARDUST🔰\n------------------------------------\n🔴 BETA SCRIPT🔴\n------------------------------------\n⚠️ INFORMATION:⚠️\n Скрипт может иметь баги, по скольку это бета.\n\n Channel: \n Telegram: @STARDUST_IO ","ОК")



 OnOff = true

 end 

OnOff = false

on = '【On】'
off = '【Off】'

gm = off
clmb = off
mgclmb = off
invsb = off
flair = off
shakjum = off
cam = off
blchm = off
grnchm = off
cmt = off
jumph = off
gravityh = off
freair = off
deadh = off
longjh = off
wallshoth = off
speedh = off

function Main()

if OnOff == false then

onCreate()
else
end

menu = gg.choice({
'Game [Menu]',
'Parametrs [Menu]',
'Chams [Menu]',
'Authors',
'Exit'
},nil, '🔰Script by STARDUST🔰 v1〔Block Strike 7.8.5〕')
if menu == nil then else
if menu == 1 then Game() end
if menu == 2 then Visuals() end
if menu == 3 then ChamsMenu() end
if menu == 4 then IAuthor() end
if menu == 5 then gg.alert("Спасибо что пользуетесь скриптом. Удачной игры.") print('Вы завершили скрипт, удачной игры!') gg.toast("🔰Script by STARDUST🔰")
os.exit() end end end

