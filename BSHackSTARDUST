function Game()
play = gg.multiChoice({
'GodMode' .. gm,
'Climb Walls' .. clmb,
'[Mega]Climb Walls' .. mgclmb,
'Invisible' .. invsb,
'Fly Air' .. flair,
'Jump Hack' .. jumph,
'Gravity' .. gravityh,
'Freeze In The Air' .. freair,
'Dead' .. deadh,
'LongJump' .. longjh,
'WallShot' .. wallshoth,
'SpeedHack' .. speedh,
'Back',
},nil, '〔Game Menu〕')
if play == nil then Main() else
if play[1] == true then God() end
if play[2] == true then Climb() end
if play[3] == true then MegaClimb() end
if play[4] == true then Invisible() end
if play[5] == true then Fly() end
if play[6] == true then JumpHack() end
if play[7] == true then Gravity() end
if play[8] == true then FreezeAir() end
if play[9] == true then Dead() end
if play[10] == true then LongJump() end
if play[11] == true then WallShot() end
if play[12] == true then SpeedHack() end
if play[13] == true then back1() end end end

function Visuals()
cham = gg.multiChoice({
'Shaking Jump' .. shakjum,
'Camera 360' .. cam,
'NickName',
'Back',
},nil, '〔Visuals Menu〕')
if cham == nil then Main() else
if cham[1] == true then Shjump() end 
if cham[2] == true then Camhk() end
if cham[3] == true then Name() end
if cham[4] == true then back1() end end end

function ChamsMenu()
chamsmenu = gg.multiChoice({
'Material Chams' .. cmt,
'Blue Chams' .. blchm,
'Green Chams' .. grnchm,
'Back',

},nil, '〔Visuals Menu〕')
if chamsmenu == nil then Main() else
if chamsmenu[1] == true then ChamsMaterial() end 
if chamsmenu[2] == true then BlueChams() end 
if chamsmenu[3] == true then GreenChams() end
if chamsmenu[4] == true then back1() end end end

